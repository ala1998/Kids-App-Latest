      "./src/assets/*",
      "./src/assets/images/*",
      "./src/assets/fonts/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.ala_elska.KidsFairyTales"
    },
    "web": {
    },
    "extra": {
      "eas": {
        "projectId": "dd5b05b4-a8c0-4e7e-a3d2-b27171d61b73"
      }
    }
  }
}