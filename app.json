{
  "expo": {
    "name": "Kids-Fairy-Tales",
    "slug": "Kids-Fairy-Tales",
    "version": "1.0.0",
    "plugins": [
      [
        "expo-facebook",
        {
          "userTrackingPermission": false
        }
      ]
    ],
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "cover"
